<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.ControleDoVitao" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/highlight_darkest</item>
        <item name="colorPrimaryVariant">@color/highlight_dark</item>
        <item name="colorOnPrimary">@color/neutral_light_lightest</item>

        <item name="colorSecondary">@color/highlight_medium</item>
        <item name="colorSecondaryVariant">@color/highlight_dark</item>
        <item name="colorOnSecondary">@color/neutral_dark_darkest</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

    <style name="CalendarTextOverride">
        <item name="android:textColor">@color/neutral_dark_darkest</item>
        <item name="android:textSize">12sp</item>
    </style>

</resources>